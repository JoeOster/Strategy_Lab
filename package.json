  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "node --watch server.js",
    "test": "npm run test:e2e >log/npm_e2e.log",
    "test:e2e": "playwright test",
    "check:biome": "biome check .",
    "check:md": "eslint . --ext .md && prettier --check \"**/*.md\"",
