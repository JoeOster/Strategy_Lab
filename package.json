    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-markdown": "^5.1.0",
    "eslint-plugin-prettier": "^5.5.4",
    "cross-env": "^7.0.3",
    "live-server": "^1.2.0",
    "playwright": "^1.56.1",
    "prettier": "^3.6.2"
  },
